{
  "formulas": [
    {
      "sheet": "Purchase Workflow Register",
      "target_column": "Planned1",
      "start_row": 3,
      "formula": "=ARRAYFORMULA(IF(ROW(A3:A)=3, \"Planned1\", IF(A3:A=\"\", \"\", TEXT(A3:A, \"dd/mm/yyyy HH:mm:ss\"))))",
      "description": "Stage 1 planned calculated from timestamp (Column A) formatted as text",
      "dependencies": [
        "Timestamp"
      ],
      "apply_to_all_rows": true,
      "is_planned_formula": true,
      "is_delay_formula": false,
      "stage_number": 1
    },
    {
      "sheet": "Purchase Workflow Register",
      "target_column": "Delay1",
      "start_row": 3,
      "formula": "=ARRAYFORMULA(IF(ROW(A3:A)=3, \"Delay1\", IF((Actual1!4:Actual1!<>\"\")*(Planned1!4:Planned1!<>\"\"), Actual1!4:Actual1!-DATEVALUE(Planned1!4:Planned1!), \"\")))",
      "description": "Stage 1 delay calculation (Actual1 - DATEVALUE(Planned1 text))",
      "dependencies": [
        "Planned1",
        "Actual1"
      ],
      "apply_to_all_rows": true,
      "is_planned_formula": false,
      "is_delay_formula": true,
      "stage_number": 1
