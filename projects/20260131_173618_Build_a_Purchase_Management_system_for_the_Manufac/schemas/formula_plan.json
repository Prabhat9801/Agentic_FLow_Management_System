{
  "formulas": [
    {
      "sheet": "Purchase Workflow Register",
      "target_column": "Planned1",
      "start_row": 4,
      "formula": "=ARRAYFORMULA(if(row(A4:A)=4,\"Planned1\",IF(A4:A=\"\",\"\",A4:A)))",
      "description": "Stage 1 planned date copied from Timestamp (col A) when record exists",
      "dependencies": [
        "Timestamp"
      ],
      "apply_to_all_rows": true,
      "is_planned_formula": true,
      "is_delay_formula": false,
      "stage_number": 1
    },
    {
      "sheet": "Purchase Workflow Register",
      "target_column": "Delay1",
      "start_row": 4,
      "formula": "=ARRAYFORMULA(if(row(A4:A)=4,\"Delay1\",if(AND(Actual14:Actual1<>\"\",Planned14:Planned1<>\"\"),Actual14:Actual1-Planned14:Planned1,\"\")))",
      "description": "Stage 1 delay = Actual1 - Planned1 (blank if either missing)",
