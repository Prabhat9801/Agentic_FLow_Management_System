    {
      "sheet": "Purchase Workflow Register",
      "target_column": "Selected Vendor Name",
      "start_row": 4,
      "formula": "=ARRAYFORMULA(IF(ROW(A4:A)=4,\"Selected Vendor Name\",IF(INDIRECT(\"RC4:RC\")=\"\",\"\",IFERROR(VLOOKUP(INDIRECT(\"RC4:RC\"),'Vendor Master'!B:C,2,FALSE),\"\"))))",
      "description": "Lookup Selected Vendor Name from Vendor Master using Selected Vendor ID (assumes Vendor Master: Vendor ID in column B, Vendor Name in column C). Uses R1C1-style INDIRECT to avoid hardcoding column letters.",
      "dependencies": [
        "Selected Vendor ID",
        "Vendor Master!Vendor ID",
        "Vendor Master!Vendor Name"
      ],
      "apply_to_all_rows": true,
      "is_planned_formula": false,
      "is_delay_formula": false,
      "stage_number": 4
    }
