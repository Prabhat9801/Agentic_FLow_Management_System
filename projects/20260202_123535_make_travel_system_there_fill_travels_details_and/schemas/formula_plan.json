    {
      "sheet": "Travel Workflow Register",
      "target_column": "Planned1",
      "start_row": 3,
      "formula": "=ARRAYFORMULA(IF(ROW(A3:A)=3, \"Planned1\", {A3:A}))",
      "description": "Stage 1 planned calculated from timestamp",
      "dependencies": [
        "Timestamp"
      ],
      "apply_to_all_rows": true,
      "is_planned_formula": true,
      "is_delay_formula": false,
      "stage_number": 1
    },
    {
      "sheet": "Travel Workflow Register",
      "target_column": "Delay1",
      "start_row": 3,
      "formula": "=ARRAYFORMULA(IF(ROW(A3:A)=3, \"Delay1\", IF(([[Actual1]]<>\"\")*([[Planned1]]<>\"\"), [[Actual1]]-[[Planned1]], \"\")))",
      "description": "Stage 1 delay calculation (days) = Actual1 - Planned1 when both present",
